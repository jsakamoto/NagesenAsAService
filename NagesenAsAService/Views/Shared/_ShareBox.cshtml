@model Room
<span> - Room #@Model.RoomNumber</span>
    |
<span class="room-url">@Model.ShortUrl</span>
    |
<a href="#" class="qr-code-icon"></a>

<div class="share-box">
    <div class="header">
        <span class="room-url">@Model.ShortUrl</span>
        <input type="button" value="Tweet" id="buttonTweet" />
    </div>
    @QRCode.Render(Model.ShortUrl, scale: 12, cssClass: "qr-code")
</div>

